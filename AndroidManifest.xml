<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.latviankeyboard"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="17" />
    
    
    <uses-permission android:name="android.permission.VIBRATE" />
    

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
        
        <activity
            android:name="com.project.latviankeyboard.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        
        
        
        <service android:name="com.project.latviankeyboard.extrarow.ExtraRowKeyboard"
                android:permission="android.permission.BIND_INPUT_METHOD"
                android:label="@string/ime_name"
                >
            <intent-filter>
                <action android:name="android.view.InputMethod" />
            </intent-filter>
            <meta-data android:name="android.view.im" android:resource="@xml/extra_row_method" />
        </service>
        
        
  <!-- COMMENT --> 
        
        
        
        <service android:name="com.project.latviankeyboard.testingstuff.TestKeyboard"
                android:permission="android.permission.BIND_INPUT_METHOD"
                android:label="test keyboard 00"
                >
            <intent-filter>
                <action android:name="android.view.InputMethod" />
            </intent-filter>
            <meta-data android:name="android.view.im" android:resource="@xml/extra_row_method" />
        </service>
        
        
        
        
    </application>

</manifest>
